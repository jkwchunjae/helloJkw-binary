<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
    </startup>
   <system.net>
      <webRequestModules>
         <add prefix = "http"
            type = "System.Net.HttpRequestCreator"
         />
         <add prefix = "https"
            type = "System.Net.HttpRequestCreator"
         />
         <add prefix = "file"
            type = "System.Net.FileWebRequestCreator"
         />
      </webRequestModules>
   </system.net>
</configuration>
