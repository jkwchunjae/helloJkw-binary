<body style="padding: 20px;">
	<h1>여긴 이름이 와야지</h1>
	<div class="ui three cards stackable">
		@for (var i = 0; i < 5; i++)
		{
			<div class="card">
				<div class="content">
					<div class="header"><a href="/workmanager/project/임용고시">프로젝트 이름</a></div>
					<div class="description">
						<div class="ui progress small teal" id="progress-@i">
							<div class="bar">
							</div>
							<div class="label">123 / 243 ( 33% )</div>
						</div>
					</div>
				</div>
			</div>
		}
	</div>
	<script src="/Static/js/jquery-2.1.3.min.js"></script>
	<script src="/Static/js/semantic-ui/semantic.js"></script>

	<script type="text/javascript">
		$('#progress-1')
		.progress({
			percent: 33
		})
	</script>
